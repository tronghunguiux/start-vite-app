{
  "name": "mypwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:dev": "sass --watch ./src/scss:./public/assets/cssmin",
    "build:prod": "sass ./src/scss:./public/assets/cssmin --style compressed --no-source-map",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prod": "npm run build:prod && next build && next start --port 4500"
  },
  "dependencies": {
    "next": "15.2.2",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/axios": "^0.14.4",
    "@types/crypto-js": "^4.2.2",
    "@types/md5": "^2.3.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "md5": "^2.3.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.14",
    "typescript": "^5"
  }
}
